# mount point	fstype		device			[device2]   fstype2
/recovery	emmc		/dev/block/mmcblk0p13
/boot		emmc		/dev/block/mmcblk0p8
/cache		ext4		/dev/block/mmcblk0p28  
/data		ext4		/dev/block/mmcblk0p30
/sdcard		vfat		/dev/block/mmcblk1p1 /dev/block/mmcblk1  flags=display="Micro SDcard";storage;wipeingui;removable
/system		ext4		/dev/block/mmcblk0p27
